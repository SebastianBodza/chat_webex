{
  "name": "example-nextjs-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "pnpm --filter @chat-adapter/webex build",
    "dev": "next dev --inspect",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "webex:webhooks": "tsx scripts/register-webex-webhooks.ts",
    "recording:list": "tsx src/lib/recorder.ts --list",
    "recording:export": "tsx src/lib/recorder.ts"
  },
  "dependencies": {
    "@chat-adapter/discord": "workspace:*",
    "@chat-adapter/gchat": "workspace:*",
    "@chat-adapter/github": "workspace:*",
    "@chat-adapter/linear": "workspace:*",
    "@chat-adapter/slack": "workspace:*",
    "@chat-adapter/state-memory": "workspace:*",
    "@chat-adapter/state-redis": "workspace:*",
    "@chat-adapter/telegram": "workspace:*",
    "@chat-adapter/teams": "workspace:*",
    "@chat-adapter/webex": "workspace:*",
    "ai": "^6.0.5",
    "chat": "workspace:*",
    "next": "^16.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redis": "^5.11.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
